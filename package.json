{
  "name": "kochii",
  "version": "0.0.2",
  "description": "Assists and encourages individuals for a manageable meal preparation lifestyle",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:app\"",
    "dev:server": "nodemon index.js",
    "dev:app": "cd kochii-app && npm run dev",
    "dev:site": "gatsby develop",
    "build:app": "cd kochii-app && npm run build",
    "build:site": "gatsby build",
    "test": "jest --silent --forceExit --detectOpenHandles --maxWorkers=10",
    "postinstall": "npm run build:site && cd kochii-app && npm install"
  },
  "keywords": [],
  "author": "jeffrey ram pineda <jeffreyrampineda@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "@sendgrid/mail": "^7.2.0",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "bootstrap": "^4.5.2",
    "concurrently": "^5.2.0",
    "crypto-random-string": "^3.2.0",
    "dotenv": "^8.1.0",
    "ejs": "^3.1.3",
    "gatsby": "^2.24.34",
    "gatsby-image": "^2.4.14",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-manifest": "^2.4.22",
    "gatsby-plugin-offline": "^3.2.22",
    "gatsby-plugin-page-creator": "^2.3.20",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.25",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-transformer-sharp": "^2.5.12",
    "jsonwebtoken": "^8.4.0",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^5.2.0",
    "koa-jwt": "^3.5.1",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-passport": "^4.1.3",
    "koa-router": "^7.4.0",
    "koa-sendfile": "^2.0.1",
    "koa-static": "^5.0.0",
    "mongoose": "^5.3.12",
    "node-sass": "^4.14.1",
    "passport-jwt": "^4.0.0",
    "prop-types": "^15.7.2",
